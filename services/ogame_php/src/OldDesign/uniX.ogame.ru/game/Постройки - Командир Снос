        }
        
        if (1) {
        	bxx.innerHTML=h+":"+m+":"+s+"<br><a href=index.php?page=b_building&session="+ps+"&listid="+pk+"&modus="+pm+"&planet="+pl+">Отменить</a>";
    	} else {
    		bxx.innerHTML=h+":"+m+":"+s+"<br><a href=index.php?page=b_building&session="+ps+"&listid="+pk+"&modus="+pm+"&planet="+pl+">Отменить</a>";
    	}
	}    
	pp=pp-1;
	if (timeout == 1) {
    	window.setTimeout("t();", 999);
    }
    }
}
