APP_NAME{% raw %}="{% endraw %}{{ LARAVEL_APP_NAME }}"
APP_ENV{% raw %}={% endraw %}{{ LARAVEL_APP_ENV }}
APP_KEY{% raw %}={% endraw %}{{ LARAVEL_APP_KEY }}
APP_DEBUG{% raw %}={% endraw %}{{ LARAVEL_APP_DEBUG }}
APP_LOG_LEVEL{% raw %}={% endraw %}{{ LARAVEL_APP_LOG_LEVEL }}
APP_URL{% raw %}={% endraw %}{{ LARAVEL_APP_URL }}

{% if LARAVEL_DB_CONNECTION is defined %}
DB_CONNECTION{% raw %}={% endraw %}{{ LARAVEL_DB_CONNECTION }}
DB_HOST{% raw %}={% endraw %}{{ LARAVEL_DB_HOST }}
DB_DATABASE{% raw %}={% endraw %}{{ LARAVEL_DB_DATABASE }}
DB_USERNAME{% raw %}={% endraw %}{{ LARAVEL_DB_USERNAME }}
DB_PASSWORD{% raw %}="{% endraw %}{{ LARAVEL_DB_PASSWORD }}"
{% else %}
DB_CONNECTION{% raw %}={% endraw %}sqlite
DB_DATABASE{% raw %}={% endraw %}:memory:
{% endif %}

BROADCAST_DRIVER{% raw %}={% endraw %}{{ LARAVEL_BROADCAST_DRIVER | default('log') }}
CACHE_DRIVER{% raw %}={% endraw %}{{ LARAVEL_CACHE_DRIVER | default('file') }}
SESSION_DRIVER{% raw %}={% endraw %}{{ LARAVEL_SESSION_DRIVER | default('file') }}
QUEUE_DRIVER{% raw %}={% endraw %}{{ LARAVEL_QUEUE_DRIVER | default('sync') }}

{% if LARAVEL_VARIABLES is defined %}
{% for item in LARAVEL_VARIABLES %}
{{ item.name }}{% raw %}="{% endraw %}{{ item.value }}"
{% endfor %}
{% endif %}